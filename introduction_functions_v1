#include <cstdio>
using namespace std;

int max_of_four(int s,int t,int u,int v){
    if(s>t && s>u && s>v){
        return s;
    }
    else if(t>s && t>u && t>v){
        return t;
    }
    else if(u>s && u>t && u>v){
        return u;
    }
    else{
        return v;
    }
}

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
