#include <iostream>
#include <algorithm> 
using namespace std;
int max_of_four(int a, int b, int c, int d) {
    return max(max(a, b), max(c, d));
}

int main() {
    int a, b, c, d;
    if (scanf("%d %d %d %d", &a, &b, &c, &d) != 4) return 1;
    printf("%d\n", max_of_four(a, b, c, d));
    return 0;
}
